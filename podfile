
target 'DachangDriverClient' do
    platform :ios,'9.0'
    # 集成动画框架
    pod 'pop', '~> 1.0.10', :inhibit_warnings => true
    # 集成 YYKit 框架
    pod 'YYKit' , '~> 1.0.9', :inhibit_warnings => true
#    # 集成 uitableviewcell 自动计算高度工具
#    pod 'UITableView+FDTemplateLayoutCell', :inhibit_warnings => true
    # 集成上／下拉刷新工具
    pod 'MJRefresh', :inhibit_warnings => true
    # 集成 masonry 布局框架
    pod 'Masonry', :inhibit_warnings => true
    # 集成 hud 显示
    pod 'MBProgressHUD', :inhibit_warnings => true
    # 集成 SDWebImage
    pod 'SDWebImage', :inhibit_warnings => true
#    # 极光推送
#    pod 'JPush', :inhibit_warnings => true
    # 腾讯 Bugly 框架
    pod 'Bugly', :inhibit_warnings => true
    # 集成 QMUI 模板
    pod 'QMUIKit', :inhibit_warnings => true
    # 百度定位
    pod 'BMKLocationKit', :inhibit_warnings => true
    #百度地图SDK
    pod 'BaiduMapKit', :inhibit_warnings => true
    # 猿题库基于 AFNetworking 封装的网络工具
    pod 'YTKNetwork', :inhibit_warnings => true
    #   数据库
    pod 'FMDB', :inhibit_warnings => true
    pod 'IQKeyboardManager' #iOS8 and later
end
